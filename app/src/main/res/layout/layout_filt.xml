<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <Button
        android:text="distinct操作符"
        android:onClick="distinct"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/distinct_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="elementAt操作符"
        android:onClick="elementAt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/elementAt_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <Button
        android:text="first操作符"
        android:onClick="first"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/first_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="last操作符"
        android:onClick="last"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/last_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="take操作符"
        android:onClick="take"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/take_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="ignoreEmelemts操作符"
        android:onClick="ignoreElements"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/ignoreElements_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <Button
        android:text="skip操作符"
        android:onClick="skip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/skip_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <Button
        android:text="filt操作符"
        android:onClick="filt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/filt_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <Button
        android:text="debounce操作符"
        android:onClick="debounce"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/debounce_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>